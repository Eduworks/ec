    <xsd:simpleType name="unit">
        <xsd:union memberTypes="acceptanceParameterUnit angleUnit areaRateUnit areaUnit binaryRateUnit binaryUnit countRateUnit countUnit currencyUnit dataTransferRateUnit electricCurrentUnit electricTensionUnit eventRateUnit eventUnit 
            forceUnit lengthRateUnit lengthUnit luminousIntensityRateUnit luminousIntensityUnit quantityUnit massUnit massRateUnit powerUnit
            relativeUnit speedUnit temperatureUnit timeConsumptionRateUnit timeConsumptionUnit timeCycleRateUnit timeCycleUnit timeRateUnit timeUnit torqueUnit volumeRateUnit volumeUnit"/>
    </xsd:simpleType>
    
    <!-- All groups of units, including unions such as timeCycleUnit, must be included in the unit union memberTypes. 
        Use and restriction of the propertyType unit UML attribute requires this -->
    
    <xsd:simpleType name="acceptanceParameterUnit">
        <xsd:union memberTypes="angleUnit electricCurrentUnit electricTensionUnit forceUnit lengthUnit luminousIntensityUnit massUnit powerUnit temperatureUnit timeUnit torqueUnit volumeUnit"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="quantityUnit">
        <xsd:union memberTypes="areaUnit countUnit lengthUnit massUnit relativeUnit volumeUnit"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="timeCycleUnit">
        <xsd:union memberTypes="eventUnit lengthUnit timeUnit"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="timeCycleRateUnit">
        <xsd:union memberTypes="eventRateUnit lengthRateUnit timeRateUnit"/>
    </xsd:simpleType>    

    
